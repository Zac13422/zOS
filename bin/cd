local tArgs = { ... }

if #tArgs < 1 then
	text.usage( "cd <path>" )
	return
end

local sNewDir = shell.resolve( tArgs[1] )
if fs.isDir( sNewDir ) then
	if (user.getActualUser()~="root")	and (sNewDir=="data/usrdata") or (user.getActualUser()~="root") and (sNewDir=="data/usrlist") then
		print("Acess denied")
	else
		shell.setDir( sNewDir )
	end
	
else
  	text.err( "Not a directory" )
  	return
end
